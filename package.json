{
    "name": "newtestingproject",
    "type": "module",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@wdio/allure-reporter": "^9.20.0",
        "@wdio/cli": "^9.20.0",
        "@wdio/cucumber-framework": "^9.21.0",
        "@wdio/devtools-service": "^10.0.1",
        "@wdio/junit-reporter": "^9.20.0",
        "@wdio/local-runner": "^9.20.0",
        "@wdio/mocha-framework": "^9.20.0",
        "@wdio/spec-reporter": "^9.20.0",
        "allure-commandline": "^2.34.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.5.0",
        "mocha": "^10.2.0",
        "prettier": "3.7.4"
    },
    "scripts": {
        "test": "wdio run ./wdio.conf.js",
        "lint": "eslint . --ext .js",
        "format": "prettier --write .",
        "test:ui": "npm test",
        "test:api": "mocha test/api/api.test.js --reporter spec",
        "test:bdd": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\"",
        "test:bdd:filter": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\" --cucumberOpts.tagExpression=\"@filter\"",
        "test:bdd:search": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\" --cucumberOpts.tagExpression=\"@search\"",
        "test:bdd:language": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\" --cucumberOpts.tagExpression=\"@language\"",
        "test:bdd:positive": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\" --cucumberOpts.tagExpression=\"@positive\"",
        "test:bdd:negative": "wdio run ./wdio.conf.js --spec \"./test/BDD/feature/*.feature\" --cucumberOpts.tagExpression=\"@negative\"",
        "allure:generate": "npx allure generate reports/allure-results --clean -o allure-report",
        "allure:open": "npx allure open allure-report",
        "allure:report": "npm run allure:generate && npm run allure:open"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "chai": "^6.2.1"
    }
}